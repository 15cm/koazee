{
  "DisableAll": true,
  "Enable": [
    "deadcode",
    "unconvert",
    "errcheck",
    "vet",
    "vetshadow",
    "gocyclo",
    "golint",
    "varcheck",
    "structcheck",
    "ineffassign",
    "interfacer",
    "unconvert",
    "goconst",
    "safesql",
    "unparam",
    "misspell",
    "megacheck",
    "test"
  ],
  "Linters": {
    "vet": "go tool vet -composites=false :PATH:LINE:MESSAGE"
  },
  "Concurrent": 5,
  "Deadline": "3m",
  "Fast": true,
  "Format": "{{.Path}}:{{.Line}}:{{if .Col}}{{.Col}}{{end}}:{{.Severity}}: {{.Message}} ({{.Linter}})",
  "Severity": {
    "goimports": "error"
  }
}
